{
	"entryFile": "src/app.ts",
	"noImplicitAdditionalProperties": "throw-on-extras",
	"controllerPathGlobs": ["src/**/*Controller.ts"],
	"spec": {
		"outputDirectory": "build",
		"specVersion": 3,
		"securityDefinitions": {
			"sessionToken": {
				"type": "http",
				"scheme": "bearer"
			}
		}
	},
	"routes": {
		"routesDir": "build",
		"authenticationModule": "./src/helpers/auth.ts"
	}
}
